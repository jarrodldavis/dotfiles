{
    "chat.editing.alwaysSaveWithGeneratedChanges": true,
    "chat.editing.confirmEditRequestRetry": false,
    "containers.containerClient": "com.microsoft.visualstudio.containers.docker",
    "dataWrangler.defaultViewingFilterOptions": "advanced",
    "dev.containers.defaultExtensions": [
        "github.copilot-chat",
        "github.copilot",
        "mathematic.vscode-pdf",
        "mattpocock.ts-error-translator",
        "ms-vscode.vscode-commander",
        "ms-vscode.vscode-copilot-data-analysis",
        "yoavbls.pretty-ts-errors"
    ],
    "dev.containers.defaultFeatures": {
        "ghcr.io/devcontainers/features/common-utils:2": {
            "configureZshAsDefaultShell": true,
            "installOhMyZsh": false,
            "installOhMyZshConfig": false,
            "installZsh": true,
            "upgradePackages": true
        },
        "ghcr.io/devcontainers/features/github-cli:1": {
            "installDirectlyFromGitHubRelease": true,
            "version": "latest"
        },
        "ghcr.io/hwaien/devcontainer-features/match-host-time-zone:0": {}
    },
    "diffEditor.experimental.showMoves": true,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "dotfiles.repository": "jarrodldavis/dotfiles",
    "dotfiles.targetPath": "~/.dotfiles",
    "editor.accessibilitySupport": "off",
    "editor.guides.bracketPairs": true,
    "editor.guides.bracketPairsHorizontal": true,
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "all",
    "editor.suggest.showStatusBar": true,
    "explorer.fileNesting.enabled": false,
    "git.blame.editorDecoration.enabled": true,
    "git.enableCommitSigning": true,
    "git.enableSmartCommit": true,
    "git.fetchOnPull": true,
    "git.openRepositoryInParentFolders": "never",
    "git.showProgress": false,
    "go.toolsManagement.autoUpdate": true,
    "gopls": {
        "ui.semanticTokens": true
    },
    "jupyter.askForKernelRestart": false,
    "jupyter.themeMatplotlibPlots": true,
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "notebook.diff.overviewRuler": true,
    "notebook.lineNumbers": "on",
    "notebook.output.scrolling": true,
    "redhat.telemetry.enabled": true,
    "remote.autoForwardPortsSource": "hybrid",
    "scm.inputFontFamily": "editor",
    "search.quickOpen.includeHistory": false,
    "svelte.enable-ts-plugin": true,
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.scrollback": 100000,
    "terminal.integrated.shellIntegration.environmentReporting": true,
    "terminal.integrated.showExitAlert": false,
    "terminal.integrated.stickyScroll.enabled": true,
    "terminal.integrated.suggest.enabled": true,
    "terminal.integrated.suggest.runOnEnter": "always",
    "totalTypeScript.hideAllTips": true,
    "totalTypeScript.hideBasicTips": true,
    "vim.camelCaseMotion.enable": true,
    "vim.easymotion": true,
    "vim.highlightedyank.enable": true,
    "vim.replaceWithRegister": true,
    "vim.sneak": true,
    "window.dialogStyle": "custom",
    "workbench.sideBar.location": "right"
}
